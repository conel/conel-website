	<Template title="Quote" image="template1.gif">
		<Description>Quote and author</Description>
		<Html>
			<![CDATA[
				<blockquote><span class="quote">&#145;Quote text&#146;</span>
				<span class="name">- Author, Year</span></blockquote>
				</blockquote>
			]]>
