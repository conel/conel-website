<!-- BEGIN customselector -->
<div id="closed_{element_name}">
<table border="0" width="300" cellspacing="0" cellpadding="0" >
	<tr>
		<td align="left" valign="top"><img src="interface/images/blank.gif" alt="" width="20" height="5" border="0"></td>
		<td align="left" valign="top"><img src="interface/images/blank.gif" alt="" width="346" height="1" border="0"></td>
	</tr>
	<tr>
		<td align="left" valign="top"><a href="#" onclick="showHideModule('closed_{element_name}','open_{element_name}');"><img src="interface/images/menu/menu_corner_plus.gif" alt="" width="20" height="17" border="0"></a></td>
		<td align="left" valign="top"><b>{LANG_DATATYPE_META_Title}</b></td>
	</tr>	
	<tr>
		<td align="left" valign="top" colspan="2"><img src="interface/images/blank.gif" alt="" width="1" height="2" border="0"></td>
	</tr>
	<tr>
		<td align="left" valign="top" colspan="2"><img src="interface/images/seperator.gif" alt="" width="366" height="2" border="0"></td>
	</tr>
</table>
</div>
<div id="open_{element_name}" style="display:none;">
<table border="0" width="300" cellspacing="0" cellpadding="0" >
	<tr>
		<td align="left" valign="top"><img src="interface/images/blank.gif" alt="" width="20" height="5" border="0"></td>
		<td align="left" valign="top"><img src="interface/images/blank.gif" alt="" width="346" height="1" border="0"></td>
	</tr>
	<tr>
		<td align="left" valign="top"><a href="#" onclick="showHideModule('open_{element_name}','closed_{element_name}');"><img src="interface/images/menu/menu_corner_minus.gif" alt="" width="20" height="17" border="0"></a></td>
		<td align="left" valign="top"><b>{LANG_DATATYPE_META_Title}</b></td>
	</tr>	
</table>
<table border="0" width="300" cellspacing="0" cellpadding="0" >
	<tr>
		<td align="left" valign="top" rowspan="8"><img src="interface/images/blank.gif" alt="" width="20" height="10" border="0"></td>
		<td align="left" valign="top"><img src="interface/images/blank.gif" alt="" width="346" height="8" border="0"></td>
	</tr>
	<tr><td align="left" valign="top"><b>{LANG_DATATYPE_META_PageTitle}</b></td></tr>	
	<tr><td align="left" valign="top"><p><i>{LANG_DATATYPE_META_PageTitleDesc}</i></p></td></tr>		
	<tr><td align="left" valign="top"><img src="interface/images/blank.gif" alt="" width="1" height="2" border="0"></td></tr>	
	<tr><td align="left" valign="top"><p><input type="text" name="META_title" size="55" value="{META_title}"></p></td></tr>
</table>
<table border="0" width="300" cellspacing="0" cellpadding="0" >
	<tr>
		<td align="left" valign="top" rowspan="6"><img src="interface/images/blank.gif" alt="" width="20" height="10" border="0"></td>
		<td align="left" valign="top"><img src="interface/images/blank.gif" alt="" width="346" height="8" border="0"></td>
	</tr>
	<tr><td align="left" valign="top"><b>{LANG_DATATYPE_META_PageDesc}</b></td></tr>	
	<tr><td align="left" valign="top"><p><i>{LANG_DATATYPE_META_PageDescDesc}</i></p></td></tr>
	<tr><td align="left" valign="top"><img src="interface/images/blank.gif" alt="" width="1" height="5" border="0"></td></tr>	
	<tr><td align="left" valign="top"><p><textarea name="META_description" cols="54" rows="6" wrap="virtual">{META_description}</textarea></p></td></tr>	
</table>
<table border="0" width="300" cellspacing="0" cellpadding="0" >
	<tr>
		<td align="left" valign="top" rowspan="6"><img src="interface/images/blank.gif" alt="" width="20" height="10" border="0"></td>
		<td align="left" valign="top"><img src="interface/images/blank.gif" alt="" width="346" height="8" border="0"></td>
	</tr>
	<tr><td align="left" valign="top"><b>{LANG_DATATYPE_META_Keywords}</b></td></tr>
	<tr><td align="left" valign="top"><p><i>{LANG_DATATYPE_META_KeywordsDesc}</i></p></td></tr>	
	<tr><td align="left" valign="top"><img src="interface/images/blank.gif" alt="" width="1" height="5" border="0"></td></tr>	
	<tr><td align="left" valign="top"><p><textarea name="META_keywords" cols="54" rows="6" wrap="virtual">{META_keywords}</textarea></p></td></tr>
	
</table>
<table border="0" width="300" cellspacing="0" cellpadding="0" >
	<tr>
		<td align="left" valign="top"><img src="interface/images/blank.gif" alt="" width="20" height="7" border="0"></td>
		<td align="left" valign="top"><img src="interface/images/blank.gif" alt="" width="346" height="1" border="0"></td>
	</tr>
	<tr>
		<td align="left" valign="top" colspan="2"><img src="interface/images/seperator.gif" alt="" width="366" height="2" border="0"></td>
	</tr>
</table>
</div>
<!-- END customselector -->