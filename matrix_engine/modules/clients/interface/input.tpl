on(myDatatype,myIdentifier,myCounter) {	myElement = myDatatype+'_'+myIdentifier+'_'+myCounter;		if(myCounter != document.container.selectedElement.value) {		oldCounter = document.container.selectedElement.value;		myOldElement = myDatatype+'_'+myIdentifier+'_'+oldCounter; 		YAHOO.util.Dom.removeClass(myOldElement, 'images_highlight');				YAHOO.util.Dom.addClass(myElement, 'images_highlight');		document.container.selectedElement.value = myCounter;	} }</script></head><body bgcolor="#FFFFFF" text="#000000" link="#000000" background="../../interface/images/pattern.gif">	<form action="{actionURL}" method="post" name="main" enctype="multipart/form-data"><input type="hidden" name="page" value="{PAGE}"><table border="0" width="300" cellspacing="0" cellpadding="0">	<tr>		<td align="left" valign="top" rowspan="2"><img src="../clients/interface/images/blank.gif" alt="" width="15" height="1" border="0"></td>		<td align="left" valign="top"><img src="../clients/interface/images/blank.gif" alt="" width="5" height="1" border="0"></td>		<td align="left" valign="top"><img src="../clients/interface/images/blank.gif" alt="" width="500" height="15" border="0"></td>	</tr>		<tr>		<td bgcolor="#FFFFFF" align="left" valign="top"><img src="../clients/interface/images/blank.gif" alt="" width="5" height="1" border="0"></td>		<td bgcolor="#FFFFFF">			<table border="0" width="680" cellspacing="0" cellpadding="0">				<tr><td align="left" valign="top" colspan="4"><img src="../clients/interface/images/blank.gif" alt="" width="1" height="15" border="0"></td></tr>				<tr>					<td align="left" valign="top"><img src="../clients/interface/images/blank.gif" alt="" width="25" height="1" border="0"></td>					<td align="left" valign="top" class="headline" colspan="3"><b>{language_header}</b></td>				</tr>					<tr><td align="left" valign="top" colspan="4"><img src="../clients/interface/images/blank.gif" alt="" width="1" height="5" border="0"></td></tr>					<tr>					<td align=