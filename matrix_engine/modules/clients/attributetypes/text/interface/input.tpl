<!-- BEGIN attribute -->	
<td>
	<table cellpadding="0" cellspacing="0" border="0">
		<tr>
			<td align="left" colspan="1">{title}&nbsp;</td>
		</tr>
		<tr>
			<td colspan="1"><img src="../clients/interface/images/blank.gif" width="2" height="1" border="0"></td>
		</tr>
		<tr>
			<td><input type="text" name="{attribute}" size="28" value="{value}"></td>
			<td><img src="../clients/interface/images/blank.gif" width="26" height="1" border="0"></td>
			
		</tr>
	</table>
</td>
<td><img src="../clients/interface/images/blank.gif" width="10" height="1" border="0"></td>
<!-- END attribute -->	

<!-- BEGIN attribute_noedit -->	
<td>
	<table cellpadding="0" cellspacing="0" border="0">
		<tr>
			<td align="left" colspan="1" class="ghost">{title}&nbsp;</td>
		</tr>
		<tr>
			<td colspan="2"><img src="../clients/interface/images/blank.gif" width="204" height="1" border="0"></td>
		</tr>
		<tr>
			<td><i>{value}</i></td>
			<td><img src="../clients/interface/images/blank.gif" width="26" height="1" border="0"></td>
			
		</tr>
		<tr>
			<td colspan="2"><img src="../clients/interface/images/blank.gif" width="1" height="4" border="0"></td>
		</tr>		
	</table>
</td>
<td><img src="../clients/interface/images/blank.gif" width="10" height="1" border="0"></td>
<!-- END attribute_noedit -->	
