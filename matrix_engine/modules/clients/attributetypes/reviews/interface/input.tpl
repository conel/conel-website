<!-- BEGIN attribute -->	
<td>
<table border="0" width="400" cellspacing="0" cellpadding="0">
	<tr>
		<td valign="middle" width="575">Number of Reviews:</td>
		<td valign="middle" width="2"><img src="interface/images/blank.gif" width="2" height="1"></td>
		<td valign="middle" width="125">{review_count}</td>
	</tr>
	<tr><td valign="top" colspan="3"><img src="interface/images/blank.gif" width="220" height="2"></td></tr>															
	<tr><td valign="top" colspan="3" bgcolor="#DDDDDD"><img src="interface/images/blank.gif" width="220" height="1"></td></tr>


	<tr><td valign="top" colspan="3"><img src="interface/images/blank.gif" width="220" height="2"></td></tr>	
	<tr>
		<td valign="middle" width="175">Last post:</td>
		<td valign="middle" width="2"><img src="interface/images/blank.gif" width="2" height="1"></td>
		<td valign="middle" width="125" nowrap>{review_lastpost}</td>
	</tr>
	<tr><td valign="top" colspan="3"><img src="interface/images/blank.gif" width="220" height="2"></td></tr>															
	<tr><td valign="top" colspan="3" bgcolor="#DDDDDD"><img src="interface/images/blank.gif" width="220" height="1"></td></tr>

	<tr><td valign="top" colspan="3"><img src="interface/images/blank.gif" width="220" height="2"></td></tr>	
	<tr>
		<td valign="middle" width="175">Average Rating:</td>
		<td valign="middle" width="2"><img src="interface/images/blank.gif" width="2" height="1"></td>
		<td valign="middle" width="125" nowrap>{review_rating}</td>
	</tr>
	<tr><td valign="top" colspan="3"><img src="interface/images/blank.gif" width="220" height="2"></td></tr>															
	<tr><td valign="top" colspan="3" bgcolor="#DDDDDD"><img src="interface/images/blank.gif" width="220" height="1"></td></tr>
	
</table>
</td>
<td><img src="interface/images/blank.gif" width="10" height="1" border="0"></td>
<!-- END attribute -->	
