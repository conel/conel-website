<!-- BEGIN body --><!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"><html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en"><head>	<meta http-equiv="content-type" content="text/html; charset=utf-8" />	<title>College of North East London - {HEADLINE}</title>	<meta name="keywords" content="conel, college, college of haringey enfield and north east london, further education, london, north east london, short courses, esol, adult learning, employer courses" />	<meta name="description" content="The College of Haringey, Enfield and North East London" />	<meta name="robots" content="index,follow" />	<link href="/layout/css/styles.css" rel="stylesheet" type="text/css" media="all" />	{ENFIELD_CSS}	<link href="/layout/css/print.css" rel="stylesheet" type="text/css" media="print" />	<!--[if lte IE 7]><link rel="stylesheet" type="text/css" href="/layout/css/ie.css" media="all" /><![endif]-->	<script type="text/javascript" src="/layout/js/jquery-1.6.min.js"></script></head><body class="{SECTION}"><div id="acceskeys">	<div id="skiplinks">		<ul>			<li><a href="#access-content" accesskey="c">Jump to Content [Accesskey 'c']</a></li>			<li><a href="#access-navigation" accesskey="n">Jump to Navigation [Accesskey 'n']</a></li>			<li><a href="/home" accesskey="0">Jump to Homepage [Accesskey '0']</a></li>		</ul>	</div></div>	<div id="canvas">		<div id="header" class="clearfix">			<div id="banner" class="{SECTION}"><h1>The College of Haringey, Enfield and North East London</h1><span><a id="access-navigation" href="/home" name="access-navigation"></a></span></div>			<div id="navigation" class="clearfix">				<div class="fl">					<ul class="clearfix{SECTION_TOP}">{LEVEL_1}</ul>				</div>			</div>			<div id="search">				<form action="/home/search_results" method="get">					<input type="hidden" name="search" value="all" />					<div class="clearfix">						<input type="text" name="query" id="query" value="Search term" class="fl mr10" />						<input type="image" src="/layout/img/go.gif" alt="Search" class="fl" />					</div>				</form>			</div>		</div>		<div class="intro_c">			<h1>{HEADLINE}</h1>		</div>		<div class="colcont3 clearfix">			<ul class="col1{SECTION_LEFT}">{LEVEL_2}<li class="empty"><span>&nbsp;</span></li></ul>			<div class="col2">				<a name="access-content" id="access-content">&nbsp;</a>				<div class="search">					<div class="hrsearch"><hr /></div>					{TITLE}					<div class="hide">						<form method="get" action="{matrix:TARGETPAGE}">							<div class="clearfix">								<label for="keyword" class="fl">Enter your Keyword</label>								<input type="text" name="keyword" id="keyword" value="{KEYWORDS}" class="fr mr10 w190 mt5" />							</div>							<div class="hrd"><hr /></div>							<div class="clearfix">								<label for="interest" class="fl">What are your interests?</label>								<select name="interest" id="interest" class="fr mr10 w195">{SUBJECTS}</select>							</div>							<div class="hrd"><hr /></div>							<div class="clearfix">								<label for="location" class="fl">Location you wish to study at</label>								<select name="location" id="location" class="fr mr10 w195">{LOCATIONS}</select>							</div>							<div class="hrd"><hr /></div>							<input type="image" src="/layout/img/gow.gif" alt="Search" class="ml358 mt5 mb10" />						</form>					</div>					<div class="hrb"><hr /></div>				</div>				{TABS_COURSE}				{SEARCH_COURSE}				{PAGER_COURSE}				{INFO}&nbsp;			</div>			<div class="col3">				{RELATED}			</div>		</div>		<div class="hr"><hr /></div>	</div>	<div id="specialf">{FOOTER}</div>	{FOOTER_CODE}	{ENFIELD_INC}	<script type="text/javascript" src="/layout/js/jquery-1.2.5.pack.js"></script>	<script type="text/javascript" src="/layout/js/system.js"></script></body></html><!-- END body -->
