<!-- BEGIN body --><!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
<head>
	<meta http-equiv="content-type" content="text/html; charset=utf-8" />
	<title>College of North East London - {HEADLINE}</title>
	<meta name="keywords" content="conel, college, college of haringey enfield and north east london, further education, london, north east london, short courses, esol, adult learning, employer courses" />
	<meta name="description" content="The College of Haringey, Enfield and North East London" />
	<meta name="robots" content="index,follow" />
	<link href="/layout/css/styles.css" rel="stylesheet" type="text/css" media="all" />
	{ENFIELD_CSS}
	<link href="/layout/css/print.css" rel="stylesheet" type="text/css" media="print" />
	<script type="text/javascript" src="/layout/js/jquery-1.6.min.js"></script>
</head>
<body class="{SECTION}">
<div id="acceskeys">
	<div id="skiplinks">
		<ul>
			<li><a href="#access-content" accesskey="c">Jump to Content [Accesskey 'c']</a></li>
			<li><a href="#access-navigation" accesskey="n">Jump to Navigation [Accesskey 'n']</a></li>
			<li><a href="/home" accesskey="0">Jump to Homepage [Accesskey '0']</a></li>
		</ul>
	</div>
</div>
	<div id="canvas">
		<div id="header" class="clearfix">
			<div id="banner" class="{SECTION}"><h1>The College of Haringey, Enfield and North East London</h1><span><a id="access-navigation" href="/home" name="access-navigation"></a></span></div>
			<div id="navigation" class="clearfix">
				<div class="fl">
					<ul class="clearfix{SECTION_TOP}">{LEVEL_1}</ul>
				</div>
			</div>
			<div id="search">
				<form action="/home/search_results" method="get">
					<input type="hidden" name="search" value="all" />
					<div class="clearfix">
						<input type="text" name="query" id="query" value="Search term" class="fl mr10" />
						<input type="image" src="/layout/img/go.gif" alt="Search" class="fl" />
					</div>
				</form>
			</div>
		</div>
		<div class="intro_c">
			<h1>{HEADLINE}</h1>
		</div>
		<div class="colcont2 clearfix">
			<ul class="col1{SECTION_LEFT}">{LEVEL_2}<li class="empty"><span>&nbsp;</span></li></ul>
			<div class="col2">
				<div class="intro clearfix" style="margin-bottom:-30px;">

						<img src="{IMAGE.file}" width="{IMAGE.width}" height="{IMAGE.height}" alt="{IMAGE.alt}" class="fr mr13"/>
						<a name="access-content" id="access-content">&nbsp;</a>
						{INTRO}
						{LINKS}

				</div>
				<!--<div class="hrcol2"><hr /></div>-->
				<div class="clearfix">
					<div class="subc">
						{COL1}&nbsp;
					</div>
					<div class="subc">
						{COL2}&nbsp;
					</div>
					<div class="subc">
						{COL3}&nbsp;
					</div>
				</div>
			</div>
		</div>
		<div class="hr"><hr /></div>
	</div>
	<div id="specialf">{FOOTER}</div>
	{FOOTER_CODE}
	{ENFIELD_INC}
</body>
</html><!-- END body -->
