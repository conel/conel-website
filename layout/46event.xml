<?xml version="1.0"?><page>	<page_head>		<title>Conel</title>		<description> </description>		<creation_date>2008-09</creation_date>		<owner>Webmatrix</owner>	</page_head>	<page_body>		<include name="NAVIGATION" function="module_navigation" />		<include name="GLOBALS" function="module_getGlobals" />		<text name="HEADLINE">			<tag>Headline</tag>			<description> </description>				</text>		<date name="DATE" format="j F Y">			<tag>Date</tag>			<description> </description>		</date>		<text name="DATEV">			<tag>Date</tag>			<description>The format should be 25.08 - 28.08. 2008 or 25.08. 2008</description>		</text>		<text name="LOCATION" default="CONEL">			<tag>Location</tag>			<description> </description>				</text>		<subject_checkboxgroup name="SUBJECT" itemcount="3">			<tag>Subject</tag>			<description>Choose a subject for this event. It will appear as recent news on the relevant subject homepage / course page.</description>		</subject_checkboxgroup>		<checkboxgroup name="TARGET" labels="learners,employers,staff,all" options="1,2,3,4" itemcount="4">			<tag>Audience</tag>			<description>Choose an audience for this news item.</description>		</checkboxgroup>		<box name="IMAGE" template="b_image,b_video" entry="TEXT:CAPTION">			<tag>Optional image or video</tag>			<description> </description>			</box>		<copytext name="INTRO" lines="3">			<tag>Summary / introduction</tag>			<description> </description>			</copytext>		<dhtmltext name="CONTENT" lines="30" cols="50">			<tag>Content</tag>			<description>Text formats: introductory text (Heading 3), headlines (Heading 4), paragraphs (Normal)</description>			</dhtmltext>		<listview name="GALLERY" category="05gallery" template="l_gallery" scope="local" count="10" ></listview>		<box name="LINKS" template="b_link,b_linke,b_linken,b_file" boxtemplate="b_link" entry="LINK:LINK,TEXT:HEADLINE">			<tag>Optional internal or external links or file downloads</tag>			<description> </description>		</box>		<box name="INFO" template="b_info" boxtemplate="b_info" entry="TEXT:HEADLINE">			<tag>Optional contact information</tag>			<description> </description>		</box>		<divider>			<tag>Right column</tag>			<description> </description>			</divider>		<box name="RELATED" template="b_rlinks,b_rimage,b_rprofile,b_rnews,b_revents" entry="TEXT:HEADLINE">			<tag>Related items</tag>			<description>Add related links or internal advertising to this page.</description>		</box>		<listview name="FOOTER" template="l_footerh" category="00home" highlight="true" scope="global" count="1"></listview>	</page_body></page>