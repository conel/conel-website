<?xml version="1.0"?><page>	<page_head>		<title>College of North East London</title>		<description> </description>		<creation_date>2008-09</creation_date>		<owner>Webmatrix</owner>	</page_head>	<page_body>		<include name="NAVIGATION" function="module_navigation" />		<include name="GLOBALS" function="module_getGlobals" />		<text name="HEADLINE">			<tag>Position</tag>			<description> </description>				</text>		<text name="TYPE">			<tag>Type</tag>			<description>eg. Full time</description>				</text>		<text name="SALARY">			<tag>Salary</tag>			<description> </description>				</text>		<date name="CLOSINGDATE" format="d. F Y">			<tag>Closing date</tag>			<description> </description>				</date>		<text name="REF">			<tag>Reference</tag>			<description> </description>				</text>		<dhtmltext name="CONTENT" lines="30" cols="50" config="fckconfig_faqs.js">			<tag>Brief description</tag>			<description>Text formats: paragraphs (Normal)</description>			</dhtmltext>		<box name="LINKS" template="b_file" boxtemplate="b_link" entry="LINK:LINK,TEXT:HEADLINE">			<tag>Detailed description downloads</tag>			<description> </description>		</box>		<box name="INFO" template="b_info" boxtemplate="b_info" entry="TEXT:HEADLINE">			<tag>Optional contact information</tag>			<description> </description>		</box>		<!-- nkowald - 2009-10-06 - The code that outputs application links if selected is in \htdocs\matrix_engine\datatypes\extra_checkboxgroup -->		<checkboxgroup name="SHOWFORMLINKS" labels="Yes,No" options="y,n" itemcount="2" default="y">			<tag>Show Download Application Form Links</tag>			<description></description>		</checkboxgroup>		<divider>			<tag>Right column</tag>			<description> </description>			</divider>		<box name="RELATED" template="b_rlinks,b_rimage,b_rprofile,b_rnews,b_revents" entry="TEXT:HEADLINE">			<tag>Related items</tag>			<description>Add related links or internal advertising to this page.</description>		</box>		<listview name="FOOTER" template="l_footerh" category="00home" highlight="true" scope="global" count="1"></listview>	</page_body></page>
